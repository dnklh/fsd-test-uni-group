{
  "name": "fsd-test-uni-group",
  "version": "1.0.0",
  "description": "GitHub CRM - Full-stack project management system",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "concurrently \"tsc -w\" \"nodemon dist/server.js\"",
    "dev:ts": "ts-node backend/server.ts",
    "dev:full": "concurrently \"npm run dev:ts\" \"cd frontend && PORT=3001 npm start\"",
    "install:all": "npm install && cd frontend && npm install",
    "build:all": "npm run build && cd frontend && npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "github",
    "crm",
    "nodejs",
    "react",
    "typescript",
    "postgresql"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/nodemailer": "^7.0.1",
    "axios": "^1.12.2",
    "bcryptjs": "^3.0.2",
    "bull": "^4.16.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "nodemailer": "^7.0.6",
    "pg": "^8.16.3",
    "redis": "^5.8.2",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.27"
  },
  "devDependencies": {
    "@types/bcryptjs": "^3.0.0",
    "@types/bull": "^4.10.4",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.5.2",
    "concurrently": "^9.2.1",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}
